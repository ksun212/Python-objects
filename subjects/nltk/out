466     >>> brown.categories('ca01')
467     ['news']
468     >>> brown.categories(['ca01','cb01'])
469     ['editorial', 'news']
470     >>> reuters.categories('training/9865')
471     ['barley', 'corn', 'grain', 'wheat']
472     >>> reuters.categories(['training/9865', 'training/9880'])
