            [94mfor[39;49;00m i [95min[39;49;00m [96mrange[39;49;00m([96mlen[39;49;00m(pieces)):
                modified_name = [33m"[39;49;00m[33m/[39;49;00m[33m"[39;49;00m.join(pieces[:i] + [pieces[i] + [33m"[39;49;00m[33m.zip[39;49;00m[33m"[39;49;00m] + pieces[i:])
                [94mtry[39;49;00m:
                    [94mreturn[39;49;00m find(modified_name, paths)
                [94mexcept[39;49;00m [96mLookupError[39;49;00m:
                    [94mpass[39;49;00m
    
        [90m# Identify the package (i.e. the .zip file) to download.[39;49;00m
        resource_zipname = resource_name.split([33m"[39;49;00m[33m/[39;49;00m[33m"[39;49;00m)[[94m1[39;49;00m]
        [94mif[39;49;00m resource_zipname.endswith([33m"[39;49;00m[33m.zip[39;49;00m[33m"[39;49;00m):
            resource_zipname = resource_zipname.rpartition([33m"[39;49;00m[33m.[39;49;00m[33m"[39;49;00m)[[94m0[39;49;00m]
        [90m# Display a friendly error message if the resource wasn't found:[39;49;00m
        msg = [96mstr[39;49;00m(
            [33m"[39;49;00m[33mResource [39;49;00m[33m\33[39;49;00m[33m[93m[39;49;00m[33m{resource}[39;49;00m[33m\033[39;49;00m[33m[0m not found.[39;49;00m[33m\n[39;49;00m[33m"[39;49;00m
            [33m"[39;49;00m[33mPlease use the NLTK Downloader to obtain the resource:[39;49;00m[33m\n[39;49;00m[33m\n[39;49;00m[33m"[39;49;00m
            [33m"[39;49;00m[33m\33[39;49;00m[33m[31m[39;49;00m[33m"[39;49;00m  [90m# To display red text in terminal.[39;49;00m
            [33m"[39;49;00m[33m>>> import nltk[39;49;00m[33m\n[39;49;00m[33m"[39;49;00m
            [33m"[39;49;00m[33m>>> nltk.download([39;49;00m[33m'[39;49;00m[33m{resource}[39;49;00m[33m'[39;49;00m[33m)[39;49;00m[33m\n[39;49;00m[33m"[39;49;00m
            [33m"[39;49;00m[33m\033[39;49;00m[33m[0m[39;49;00m[33m"[39;49;00m
        ).format(resource=resource_zipname)
        msg = textwrap_indent(msg)
    
        msg += [33m"[39;49;00m[33m\n[39;49;00m[33m  For more information see: https://www.nltk.org/data.html[39;49;00m[33m\n[39;49;00m[33m"[39;49;00m
    
        msg += [33m"[39;49;00m[33m\n[39;49;00m[33m  Attempted to load [39;49;00m[33m\33[39;49;00m[33m[93m[39;49;00m[33m{resource_name}[39;49;00m[33m\033[39;49;00m[33m[0m[39;49;00m[33m\n[39;49;00m[33m"[39;49;00m.format(
            resource_name=resource_name
        )
    
