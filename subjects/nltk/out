                root = nltk.data.find([33mf[39;49;00m[33m"[39;49;00m[33m{[39;49;00m[96mself[39;49;00m.subdir[33m}[39;49;00m[33m/[39;49;00m[33m{[39;49;00mzip_name[33m}[39;49;00m[33m"[39;49;00m)
