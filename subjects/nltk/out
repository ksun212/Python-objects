            [33m"[39;49;00m[33mResource [39;49;00m[33m\33[39;49;00m[33m[93m[39;49;00m[33m{resource}[39;49;00m[33m\033[39;49;00m[33m[0m not found.[39;49;00m[33m\n[39;49;00m[33m"[39;49;00m
            [33m"[39;49;00m[33mPlease use the NLTK Downloader to obtain the resource:[39;49;00m[33m\n[39;49;00m[33m\n[39;49;00m[33m"[39;49;00m
            [33m"[39;49;00m[33m\33[39;49;00m[33m[31m[39;49;00m[33m"[39;49;00m  [90m# To display red text in terminal.[39;49;00m
            [33m"[39;49;00m[33m>>> import nltk[39;49;00m[33m\n[39;49;00m[33m"[39;49;00m
            [33m"[39;49;00m[33m>>> nltk.download([39;49;00m[33m'[39;49;00m[33m{resource}[39;49;00m[33m'[39;49;00m[33m)[39;49;00m[33m\n[39;49;00m[33m"[39;49;00m
            [33m"[39;49;00m[33m\033[39;49;00m[33m[0m[39;49;00m[33m"[39;49;00m
        ).format(resource=resource_zipname)
        msg = textwrap_indent(msg)
    
